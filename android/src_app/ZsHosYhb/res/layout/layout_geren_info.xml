                    android:textSize="@dimen/textSize_small" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text=":"
                    android:textColor="@color/black"
